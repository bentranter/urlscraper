{
  "name": "urlscraper",
  "version": "1.0.0",
  "description": "URL Scraper",
  "scripts": {
    "css": "NODE_ENV=production ./node_modules/.bin/postcss app/assets/stylesheets/style.css -o public/css/style.css",
    "js": "NODE_ENV=production ./node_modules/.bin/esbuild app/assets/js/index.js --bundle --minify --sourcemap --target=chrome60,firefox60,safari11,edge18 --outfile=public/js/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bentranter/urlscraper.git"
  },
  "keywords": [
    "job scraper"
  ],
  "author": "Ben Tranter",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bentranter/urlscraper/issues"
  },
  "homepage": "https://github.com/bentranter/urlscraper#readme",
  "dependencies": {
    "@hotwired/turbo": "7.0.1",
    "@hotwired/stimulus": "3.0.1"
  },
  "devDependencies": {
    "@tailwindcss/forms": "0.3.2",
    "autoprefixer": "10.2.6",
    "esbuild": "0.13.12",
    "postcss": "8.3.0",
    "postcss-cli": "8.3.1",
    "tailwindcss": "2.2.19"
  }
}
